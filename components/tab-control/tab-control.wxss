  background-color: #fff;
  padding: 10rpx 0;
}
.tab-control-item{
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  padding: 20rpx 0;
}
.active{
  color: #ff5777;
  padding: 10rpx;
  border-bottom: 4rpx solid #ff5777;
}