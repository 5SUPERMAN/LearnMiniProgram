<!--pages/home/home.wxml-->
<!-- 1.navigator页面跳转 -->
<navigator url="/pages/detail/detail">跳转到详情页</navigator>

<!-- 2.navigator属性：open-type -->
<navigator url="/pages/detail/detail" open-type="redirect">
  跳转到详情页(redirect)
</navigator>
<navigator url="/pages/about/about" open-type="switchTab">
  跳转到关于页(switchTab)
</navigator>
<navigator url="/pages/detail/detail" open-type="reLaunch">
  跳转到详情页(reLaunch)
</navigator>

<!-- 3.跳转过程中数据的传递 -->
<navigator url="/pages/detail/detail?name=5superman&&age=3">
  跳转到详情页
</navigator>
<view>{{title}}</view>

<!-- 4.通过代码进行页面跳转 -->
<button size="mini" bind:tap="handlePushDetail">跳转到详情页</button>